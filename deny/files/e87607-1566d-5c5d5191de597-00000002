function paint_card_checkbox_bloc(card_text = '') {
	var no_checked = '';
	var yes_checked = 'checked';
	if(card_text == ''){
		no_checked = 'checked'; // a revoir
		yes_checked = '';
	} 
	return '<div class=""orders_bloc"">\
				<div class=""orders_title"">'+appString('attach_a_card')+'</div>\
				<div class=""checkbox_card"">\
					<input type=""checkbox"" value=""yes"" '+yes_checked+'/> '+appString('yes')+' <input type=""checkbox"" value=""no"" '+no_checked+'/> '+appString('no')+'\
				</div>\
			</div>';
}

