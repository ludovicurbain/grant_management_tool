173266869,crm - reminder // event sans serie ne sont pas visible a la notification,"",45,"Jun 8, 2020","Jun 21, 2020",bug,,accepted,"Jun 10, 2020","Jun 12, 2020",,VincentDG,,https://www.pivotaltracker.com/story/show/173266869,joachim Urbain,,,,,,,,,"ancienne Query: 

SELECT * FROM (
	SELECT
		t1.*,
		'client' AS relation
	FROM
		event t1
	INNER JOIN
		r_event_series r5
	ON
		t1.id=r5.id_1
	INNER JOIN
		r_event_series r6
	ON
		r6.id_2=r5.id_2
	INNER JOIN
		r_event_client r1
	ON
		r1.id_1=r6.id_1
	INNER JOIN
		r_erp_account_client r2
	ON
		r2.id_2=r1.id_2
	INNER JOIN
		erp_account t3
	ON
		r2.id_1=t3.id
	WHERE
		t3.id=$1
	UNION
	SELECT 
		t5.*,
		'document' AS relation
	FROM
		document t1
	INNER JOIN
		r_document_client r1
	ON
		r1.id_1=t1.id
	INNER JOIN
		r_erp_account_client r2
	ON
		r2.id_2=r1.id_2
	INNER JOIN
		r_document_address r3
	ON
		r3.id_1=t1.id
	INNER JOIN
		r_event_document r4
	ON
		r4.id_2=t1.id
	INNER JOIN
		r_event_series r5
	ON
		r5.id_1=r4.id_1
	INNER JOIN
		r_event_series r6
	ON
		r6.id_2=r5.id_2
	INNER JOIN
		event t5
	ON
		r6.id_1=t5.id
	WHERE
		r2.id_1=$1
) a
ORDER BY a.start_date; (joachim Urbain - Jun 11, 2020)",,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,
