function paint_lines_products(related_data) {
	var lines ='';//a split une fois la version de kedo merge
	if (related_data['product'].length != 0){
		for (var i=0; i<related_data['product'].length;i++) {
			lines += '<div id=""'+related_data['product'][i]['id']+'""><input type=""number"" value=""'+related_data['product'][i]['link.quantity']+'""/> x '+ related_data['product'][i]['description'] +'<div class=""button delete_product_line"" iid=""'+related_data['product'][i]['id']+'""></div></div>';
		}
	}
	$('.product_order_container').html(lines);
}


